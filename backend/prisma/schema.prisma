generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model criminal_records {
  id           Int       @id @default(autoincrement())
  name         String?
  dob          DateTime? @db.Date
  gender       String?
  national_id  String?
  location     String?
  offense      String?
  offense_date DateTime? @db.Date
  status       String?
  officer      String?
  case_id      String?
  sentence     String?
}
